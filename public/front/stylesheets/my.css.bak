.message-nav {
    background-color: white;;
    overflow: hidden;
    display: table;
    width: 100%;
    font-size: 15px;
	border-bottom: 1px solid #E3E3E3;
}

.message-nav li {
    display: table-cell;
    vertical-align: top;
    width: 14.96%;
    text-align: center;
    position: relative;
}

.message-nav .cur a {
    font-weight: 500;
    color: #000000;

}

.message-nav a {
    -ms-user-select: none;
    vertical-align: top;
    text-decoration: none;
    color: #000000;
    position: relative;
    display: inline-block;
    font-size: 17px;
    font-weight: 700;
    margin-bottom: 10px;
    margin-top: 10px;
    float: left;
    text-align: center;
    width: 100%;
}
.message-nav a:after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    border-right: 1px solid #E4E4E4;
}

.message-nav .cur:after {
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    border-bottom: 3px solid #ffc028;
}

.profile-nav a:after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    border-right: 1px solid #E4E4E4;
}

