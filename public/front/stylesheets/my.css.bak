.article-loading {
    position: fixed;
    margin: -45px 0 0 -73px;
    padding: 0 10px;
    min-width: 126px;
    height: 90px;
    top: 50%;
    left: 50%;
    background: rgba(46, 50, 56, .9);
    -webkit-border-radius: 4px;
    border-radius: 4px;
    z-index: 999;
}

.article-loading p {
    color: #c8beb2;
    text-align: center;
    font-size: 14px;
    padding: 18px 0 10px;
}

.article-loading figure {
    width: 20px;
    height: 20px;
    margin: 0 auto;
    -webkit-animation: daisycircle 1s linear infinite;
}

.article-loading figure {
    background: url("/public/front/images/loading6.gif") no-repeat center;
    -webkit-background-size: 20px auto;
