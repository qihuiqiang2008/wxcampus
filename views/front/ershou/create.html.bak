    <meta name="viewport" content="width=319.9,initial-scale=1,minimum-scale=1, maximum-scale=1, user-scalable=no"
          media="(device-height: 568px)">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="x5-fast-scroller" content="disable">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="stylesheet" id="CssGaga" href="/public/front/stylesheets/photo_guess_create.css?v=1210">
    <script src="http://libs.baidu.com/jquery/1.9.1/jquery.min.js" type="text/javascript"></script>
    <!--<script type="text/javascript" src="./jingxuan_files/stats" charset="UTF-8"></script>-->
    <style id="clearly_highlighting_css" type="text/css">
        /* selection */
        html.clearly_highlighting_enabled ::-moz-selection {
            background: rgba(246, 238, 150, 0.99);
        }

        html.clearly_highlighting_enabled ::selection {
            background: rgba(246, 238, 150, 0.99);
        }
        /* cursor */
        html.clearly_highlighting_enabled {
            /* cursor and hot-spot position -- requires a default cursor, after the URL one */
            cursor: url("chrome-extension://pioclpoplcdbaefihamjohnefbikjilc/clearly/images/highlight--cursor.png") 14 16, text;
        }
        /* highlight tag */
        em.clearly_highlight_element {
            font-style: inherit !important;
            font-weight: inherit !important;
            background-image: url("chrome-extension://pioclpoplcdbaefihamjohnefbikjilc/clearly/images/highlight--yellow.png");
            background-repeat: repeat-x;
            background-position: top left;
            background-size: 100% 100%;
        }
        /* the delete-buttons are positioned relative to this */
        em.clearly_highlight_element.clearly_highlight_first {
            position: relative;
        }
        /* delete buttons */
        em.clearly_highlight_element a.clearly_highlight_delete_element {
            display: none;
            cursor: pointer;
            padding: 0;
            margin: 0;
            line-height: 0;
            position: absolute;
            width: 34px;
            height: 34px;
            left: -17px;
            top: -17px;
            background-image: url("chrome-extension://pioclpoplcdbaefihamjohnefbikjilc/clearly/images/highlight--delete-sprite.png");
            background-repeat: no-repeat;
            background-position: 0px 0px;
        }

        em.clearly_highlight_element a.clearly_highlight_delete_element:hover {
            background-position: -34px 0px;
        }

        /* retina */
        @media (min--moz-device-pixel-ratio: 2), (-webkit-min-device-pixel-ratio: 2), (min-device-pixel-ratio: 2) {
            em.clearly_highlight_element {
                background-image: url("chrome-extension://pioclpoplcdbaefihamjohnefbikjilc/clearly/images/highlight--yellow@2x.png");
            }

            em.clearly_highlight_element a.clearly_highlight_delete_element {
                background-image: url("chrome-extension://pioclpoplcdbaefihamjohnefbikjilc/clearly/images/highlight--delete-sprite@2x.png");
                background-size: 68px 34px;
            }
        }
        .must_anwser{
            display: none;
        }
    </style>

    <style>
        [touch-action="none"] {
            -ms-touch-action: none;
            touch-action: none;
        }

        [touch-action="pan-x"] {
            -ms-touch-action: pan-x;
            touch-action: pan-x;
        }

        [touch-action="pan-y"] {
            -ms-touch-action: pan-y;
            touch-action: pan-y;
        }

        [touch-action="scroll"], [touch-action="pan-x pan-y"], [touch-action="pan-y pan-x"] {
            -ms-touch-action: pan-x pan-y;
            touch-action: pan-x pan-y;
        }
        .myButton {
            -moz-box-shadow: 0px 1px 0px 0px #f0f7fa;
            -webkit-box-shadow: 0px 1px 0px 0px #f0f7fa;
            box-shadow: 0px 1px 0px 0px #f0f7fa;
            background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #33bdef), color-stop(1, #019ad2));
            background:-moz-linear-gradient(top, #33bdef 5%, #019ad2 100%);
            background:-webkit-linear-gradient(top, #33bdef 5%, #019ad2 100%);
            background:-o-linear-gradient(top, #33bdef 5%, #019ad2 100%);
            background:-ms-linear-gradient(top, #33bdef 5%, #019ad2 100%);
            background:linear-gradient(to bottom, #33bdef 5%, #019ad2 100%);
            filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bdef', endColorstr='#019ad2',GradientType=0);
            background-color:#33bdef;
            -moz-border-radius:6px;
            -webkit-border-radius:6px;
            border-radius:6px;
            border:1px solid #057fd0;
            display:inline-block;
            cursor:pointer;
            color:#ffffff;
            font-family:arial;
            font-size:15px;
            font-weight:bold;
            padding:9px 76px;
            text-decoration:none;
            text-shadow:0px -1px 0px #5b6178;
        }
        .myButton:hover {
            background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #019ad2), color-stop(1, #33bdef));
            background:-moz-linear-gradient(top, #019ad2 5%, #33bdef 100%);
            background:-webkit-linear-gradient(top, #019ad2 5%, #33bdef 100%);
            background:-o-linear-gradient(top, #019ad2 5%, #33bdef 100%);
            background:-ms-linear-gradient(top, #019ad2 5%, #33bdef 100%);
            background:linear-gradient(to bottom, #019ad2 5%, #33bdef 100%);
            filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#019ad2', endColorstr='#33bdef',GradientType=0);
            background-color:#019ad2;
        }
        .myButton:active {
            position:relative;
            top:1px;
        }

    </style>

</head>
<body class="PE">
<div id="main">
    <header class="header">
        <div id="sub_nav" class="sub-nav" data-showtopbar="1" style="height:45px">
            <!--<span class="col-l"><button data-hook="subNav-back" class="b-bak js-link" onclick="goback()">返回-->
        <!--</button></span><span class="col-r"><button class="b-act js-link" link="/signin">-->
        <!--</button></span>-->
            <div id="sub_tab" class="col-m"></div>
            <h2 id="sub_title" class="hd" style="line-height:45px;font-size: 18px;font-weight: bold">杂货街</h2></div>

    </header>
    <div id="page-content" class="feed-page" style="display: block;">
        <div class="page-wrap" id="page-profile" style="display: block;">
            <div class="profile-detail" id="J_Profile">
                <div class="table-wrap">
                    <form action="/front/photo_guess/create" method="post" onsubmit="return checkform()" style="background-color:#fcfcfc">
                        <ul class="ui-table ui-table-list">
						 <li>
                                <label>类型</label>
                                <div class="select">
                                    <select name="source" id="source">
                                        <option value="-1"  selected="selected">请选择类型
                                        </option>
                                        <option value="qiugou" >求购
                                        </option>
                                        <option value="chushou">出售
                                        </option>

                                   </select>
                                </div>
                            </li>
                            <li class="active" id="picture_li">
                                <label>物品照片</label>
                                <div class="add-pic">
                                    <ul class="J_photo_container">
                                        <li id="img_upload_li" class="J_photo added-pic"
                                            style="position:relative;background:none;display: none"><span
                                                class="J_loading ui-loading" style="display: none;"><i class="t1"></i><i
                                                class="t2"></i><i class="t3"></i></span>

                                            <div class="J_loading"
                                                 style="position: absolute; width: 100%; height: 100%; display: none; background: rgba(0, 0, 0, 0.4);">
                                                <i class="loading-s-bg loading-animate"
                                                   style="position:absolute; top:50%;margin-left:-10px;margin-top:-10px; left:50%;"></i>
                                            </div>
                                            <span class="J_icon loading"><i class="icon-hook"></i></span> <img
                                                    class="J_img img" id="img_upload"/></li>
                                        <li class="J_button_photo"
                                            style="position:relative; overflow:hidden; float:left;">
                                            <div hide-focus="true"
                                                 style="opacity:0;display: block; height: 69px;  width: 69px;  position: absolute;z-index: 2;overflow:hidden;">
                                                <input class="J_button" id="J_button" type="file"
                                                       accept="image/jpeg,image/png,image/gif,image/bmp"
                                                       style="display: block; background:transparent; width: 207px; height:69px;margin-left:-69px;">
                                            </div>
                                            <div class="add-btn" style="z-index: 1;"><i class="plus"></i></div>
                                        </li>
                                    </ul>
                                    <input type="hidden" name="photo_url" id="photo_url"/>

                                </div>
                                <div>  <span style="font-size: 12px;color:#808080">传照片失败?点击右上方菜单在浏览器打开</span>
                                </div>
                            </li>
                           
                              <input type="hidden" value="<%=school.region_code%>" id="region_code" name="region_code">
                            <input type="hidden" value="<%=school.en_name%>" id="en_name" name="en_name">
                            <input type="hidden" value="<%=school.cn_name%>" id="cn_name" name="cn_name">
                            <li class="active">
                                <label id="des_title">描述</label>
                                <div>
                                    <textarea name="introduction" id="introduction" required="required" placeholder="认真描述一下" style="width: 100%;min-height:50px;border: none;font-size: 16px;padding:3;padding-top:10px;;border-radius: 0;white-space: pre-wrap;
word-wrap: break-word;"></textarea>
                                </div>
                            </li>
                            <li>
                                <label id="des_price">价格</label>
                                <div>
                                    <input type="text" value="" id="price"  name="price" placeholder="写一个大致的价格范围">
                                </div>
                            </li>                 

                            <li>
                                <label>微信ID</label>
                                <div>
                                    <input type="text" value="" id="wx_account" required="required"  name="wx_account" placeholder="填写微信ID">
                                </div>
                            </li>
                            
                            
                            
                            <li>
                                <label>可见范围</label>
                                <div class="select">
                                    <select name="range" id="range">
                                        <option value="-1" selected="selected">请选择可见范围
                                        </option>
                                        <option value="1">附近高校可见
                                        </option>
                                        <option value="0">只有本校可见
                                        </option>
                                    </select>
                                </div>
                            </li>
                            

                            
                        </ul>
                        <div style="text-align: center;padding: 15px;"><input class="mysubmit myButton" type="submit"
                                                                              value="确定提交">
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="/public/front/javascripts/localResizeIMG.js" type="text/javascript"></script>
    <!-- mobileBUGFix.js 兼容修复移动设备 -->
    <script src="/public/front/javascripts/mobileBUGFix.mini.js" type="text/javascript"></script>
    <script>
        var checkSubmitFlg = false;
        var img = new Image();
        var imagedata;
        $("#J_button").localResizeIMG({
            width: 400,
            quality: 0.7,
            success: function (result) {
                img.src = result.base64;
                imagedata = result.base64;
                $("#img_upload_li").show();
                $("#photo_url").val(imagedata);
                $("#img_upload").attr("src", result.base64);
            }
        });
        $("#source").change(function(){
            if( $("#source").val()=="qiugou"){
                $("#des_price").text("期望价格");
                $("#picture_li").hide();
             }else{
                 $("#des_price").text("价格");
                $("#picture_li").show();
					  
			}
        });
        function checkform(){
         if(!imagedata){
			 if($("#source").val()=="chushou"){
		     alert("请先上传照片");
             return false;
			 }
         }

            
            if($("#wx_account").val()==""){
                alert("微信号必填项！");
                return false;
            }
            if($("#range").val()=="-1"){
                alert("请选择可见范围");
                return false;
            }
            if(confirm("您确定您的照片和微信号是正确有效的么？")){
                return true
            }else{
                return false
            }
            if (!checkSubmitFlg) {
                checkSubmitFlg = true;
                return true;
            }else{
                alert("不用重复提交啦");
                return false;
            }

        }
        function goback(){
            window.history.back();
        }
        var temp;
        var questions=eval($("#questions").val());
        $("#question1").change(function(){
            if(temp){
                $("#question2").append(temp);
            }
           temp=$("#question2 option[value='"+$("#question1").val()+"']").remove();
        });
        $("#question2").change(function(){
            if($("#question1").val()==-1){
                return alert("先选择问题一");
            }
        });
        $("#must_anwser").change(function(){
            if($(this).val()=="1"){
                $(".must_anwser").hide();
            }else{
                $(".must_anwser").show();
            }
        })
      
    </script>

</div>
</div>
</body>
</html>