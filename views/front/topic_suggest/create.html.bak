background-color: #1d8aff;
color: #fff;
font-family: 'Microsoft YaHei';
margin: 0 auto;
display: block;
font-size: 16px;
-webkit-appearance: none;
}
input[type="text"], .j_myOperating input[type="password"], .j_myOperating input[type="number"], .j_myOperating input[type="email"], .j_myOperating input[type="tel"] {
background: #fff;
box-shadow: none;
width: 65%;
line-height: normal;
font-size: 18px;
font-family: 'Microsoft YaHei';
}
input {
border: 0;
}
input, button, select, textarea {
