<body class="PE">
<div id="main">
    <header class="header">
        <div id="sub_nav" class="sub-nav" data-showtopbar="1" style="height:45px">
            <!--<span class="col-l"><button data-hook="subNav-back" class="b-bak js-link" onclick="goback()">返回-->
        <!--</button></span><span class="col-r"><button class="b-act js-link" link="/signin">-->
        <!--</button></span>-->
            <div id="sub_tab" class="col-m"></div>
            <h2 id="sub_title" class="hd" style="line-height:45px;font-size: 18px;font-weight: bold">找缘分</h2></div>

    </header>
    <div id="page-content" class="feed-page" style="display: block;">
        <div class="page-wrap" id="page-profile" style="display: block;">
            <div class="profile-detail" id="J_Profile">
                <div class="table-wrap">
                    <form action="/front/photo_guess/create" method="post" onsubmit="return checkform()" style="background-color:#fcfcfc">
                        <ul class="ui-table ui-table-list">
                            <li class="active">
                                <label>上传照片</label>
                                <div class="add-pic">
                                    <ul class="J_photo_container">
                                        <li id="img_upload_li" class="J_photo added-pic"
                                            style="position:relative;background:none;display: none"><span
                                                class="J_loading ui-loading" style="display: none;"><i class="t1"></i><i
                                                class="t2"></i><i class="t3"></i></span>

                                            <div class="J_loading"
                                                 style="position: absolute; width: 100%; height: 100%; display: none; background: rgba(0, 0, 0, 0.4);">
                                                <i class="loading-s-bg loading-animate"
                                                   style="position:absolute; top:50%;margin-left:-10px;margin-top:-10px; left:50%;"></i>
                                            </div>
                                            <span class="J_icon loading"><i class="icon-hook"></i></span> <img
                                                    class="J_img img" id="img_upload"/></li>
                                        <li class="J_button_photo"
                                            style="position:relative; overflow:hidden; float:left;">
                                            <div hide-focus="true"
                                                 style="opacity:0;display: block; height: 69px;  width: 69px;  position: absolute;z-index: 2;overflow:hidden;">
                                                <input class="J_button" id="J_button" type="file"
                                                       accept="image/jpeg,image/png,image/gif,image/bmp"
                                                       style="display: block; background:transparent; width: 207px; height:69px;margin-left:-69px;">
                                            </div>
                                            <div class="add-btn" style="z-index: 1;"><i class="plus"></i></div>
                                        </li>
                                    </ul>
                                    <input type="hidden" name="photo_url" id="photo_url"/>

                                </div>
                                <div>  <span style="font-size: 12px;color:#808080">传照片失败?点击右上方菜单在浏览器打开</span>
                                </div>
                            </li>
                            <li>
                                <label>缘分类型</label>
                                <div class="select">
                                    <select name="source" id="source">
                                        <option value="-1"  selected="selected">请选择缘分类型
                                        </option>
                                        <option value="shiyou" >帮室友找缘分
                                        </option>
                                        <option value="ziji">帮自己找缘分
                                        </option>
                                   </select>
                                </div>
                            </li>
                              <input type="hidden" value="<%=school.region_code%>" id="region_code" name="region_code">
                            <input type="hidden" value="<%=school.en_name%>" id="en_name" name="en_name">
                            <input type="hidden" value="<%=school.cn_name%>" id="cn_name" name="cn_name">
                            <li class="active">
                                <label id="des_title">室友印象</label>
                                <div>
                                    <textarea name="introduction" id="introduction" required="required" placeholder="认真描述一下你的室友吧,包括性格、爱好、特点等等,文字不能太少！！" style="width: 100%;min-height:50px;border: none;font-size: 16px;padding:3;padding-top:10px;;border-radius: 0;white-space: pre-wrap;
word-wrap: break-word;"></textarea>
                                </div>
                            </li>
                            <li class="active">
                                <label id="des_demand">有啥要求</label>
                                <div>
                                    <textarea name="demand" id="demand" required="required" placeholder="有啥要求,文字不能太少！！" style="width: 100%;min-height:50px;border: none;font-size: 16px;padding:3;padding-top:10px;;border-radius: 0;white-space: pre-wrap;
word-wrap: break-word;"></textarea>
                                </div>
                            </li>
                            <li>
                                <label id="desc_major">室友专业：</label>
                                <div>
                                    <input type="text" value="" id="major" required="required"  name="major" placeholder="专业室友的是？">
                                </div>
                            </li>

                            <li>
                                <label>微信ID</label>
                                <div>
                                    <input type="text" value="" id="wx_account" required="required"  name="wx_account" placeholder="填真实微信号而非电话号码或QQ">
                                </div>
                            </li>
                            <li style="display: none">
                                <label>性别</label>
                                <div class="select">
                                    <select name="sex">
                                        <option value="girl" selected="selected">女生
                                        </option>
                                        <option value="boy" selected="selected">男生
                                        </option>
                                    </select>
                                </div>
                            </li>
                            <li>
                                <label>所在年级</label>
                                <div class="select">
                                    <select name="grade" id="grade">
                                        <option value="-1" selected="selected">请选择所在年级
                                        </option>
                                        <option value="大一">大一
                                        </option>
                                        <option value="大二">大二
                                        </option>
                                        <option value="大三" >大三
                                        </option>
                                        <option value="大四" >大四
                                        </option>
                                        <option value="研一" >研一
                                        </option>
                                        <option value="研二">研二
                                        </option>
                                        <option value="研三">研三
                                        </option>
                                        <option value="博士">博士
                                        </option>
                                        <option value="已毕业">已毕业
                                        </option>
                                    </select>
                                </div>
                            </li>
                            <li class="active">
                                <label  id="hometown_title">室友家乡</label>

                                <div>
                                    <input type="text" required="required" value="" id="hometown" name="hometown"
                                           placeholder="这里填家乡">
                                </div>
                            </li>
                            <li class="active" id="recommand_li">
                                <label>你是谁?</label>

                                <div>
                                    <input type="text" required="required" value="" id="recommand_name" name="recommand_name"
                                           placeholder="推荐人姓名（真实姓名），我们不接受雷锋的推荐">
                                </div>
                            </li>
                            <li>
                                <label>可见范围</label>
                                <div class="select">
                                    <select name="range" id="range">
                                        <option value="-1" selected="selected">请选择可见范围
                                        </option>
                                        <option value="1">附近高校可见
                                        </option>
                                        <option value="0">只有本校可见
                                        </option>
                                    </select>
                                </div>
                            </li>
                            <li>
                                <label>其他限制</label>
                                <div class="select">
                                    <select name="must_anwser" id="must_anwser">
                                        <option value="-1" selected="selected">请选择微信号限制
                                        </option>
                                        <option value="0">回答对问题者可见微信号(女生推荐)
                                        </option>
                                        <option value="1">微信号全部可见
                                        </option>

                                    </select>
                                </div>
                            </li>

                            <li class="must_anwser">
                                <label>问题一</label>
                                <div class="select">
                                    <select name="question1" id="question1">
                                        <option value="-1" selected="selected">设置微信号密保问题一
                                        </option>
                                        <% questions.forEach(function(question,index) { %>
                                        <option value="<%=question._id%>"><%=question.content%>
                                        </option>
                                        <% }) %>
                                    </select>
                                </div>
                            </li>
                            <li class="active must_anwser">
                                <label>答案</label>

                                <div>
                                    <input type="text" value="" id="question1_answer" name="question1_answer"
                                           placeholder="2到6个汉字">
                                </div>
                            </li>
                            <li class="must_anwser">
                                <label>问题二</label>

                                <div class="select">
                                    <select name="question2" id="question2">
                                        <option value="-1" selected="selected">设置微信号密保问题二
                                        </option>
                                        <% questions.forEach(function(question,index) { %>
                                        <option value="<%=question._id%>"><%=question.content%>
                                        </option>
                                        <% }) %>
                                    </select>
                                </div>
                            </li>
                            <li class="active must_anwser">
                                <label>答案</label>
                                <div>
                                    <input type="text" value="" id="question2_answer" name="question2_answer" placeholder="2到6个汉字">
                                </div>
                            </li>
                        </ul>
                        <div style="text-align: center;padding: 15px;"><input class="mysubmit myButton" type="submit"
                                                                              value="确定提交">
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="/public/front/javascripts/localResizeIMG.js" type="text/javascript"></script>
    <!-- mobileBUGFix.js 兼容修复移动设备 -->
    <script src="/public/front/javascripts/mobileBUGFix.mini.js" type="text/javascript"></script>
    <script>

    </script>
    <script>
        var checkSubmitFlg = false;
        var img = new Image();
        var imagedata;
        $("#J_button").localResizeIMG({
            width: 400,
            quality: 0.7,
            success: function (result) {
                img.src = result.base64;
                imagedata = result.base64;
                $("#img_upload_li").show();
                $("#photo_url").val(imagedata);
                $("#img_upload").attr("src", result.base64);
            }
        });
        $("#source").change(function(){
            if( $("#source").val()=="shiyou"){
                $("#des_title").text("室友印象");
                $("#desc_major").text("室友专业");
                $("#hometown_title").text("室友家乡");
                $("#recommand_li").show();
                $("#introduction").attr("placeholder","认真描述一下你的室友吧,包括性格、爱好、特点等等,文字不能太少！！");
                $("#major").attr("placeholder","您室友的专业是?！");
            }else{
                $("#des_title").text("自我介绍");
                $("#hometown_title").text("自己家乡");
                $("#desc_major").text("自己专业");
                $("#introduction").attr("placeholder","认真介绍一下自己吧,包括性格、爱好、特点等等,文字不能太少！！");
                $("#major").attr("placeholder","自己的专业是?！");
                $("#recommand_li").hide();
            }
        });
        function checkform(){
         if(!imagedata){
             alert("请先上传照片");
             return false;
         }

            if($("#source").val()=="-1"){
                alert("请选择缘分类型！");
                return false;
            }
            if($("#wx_account").val()==""){
                alert("微信号必填项！");
                return false;
            }
            if($("#major").val()==""){
                alert("专业必须填！");
                return false;
            }
            if($("#demand").val()==""){
                alert("要求必须填！");
                return false;
            }
            if($("#grade").val()=="-1"){
                alert("请选择所在年级");
                return false;
            }
            if($("#range").val()=="-1"){
                alert("请选择可见范围");
                return false;
            }

            if($("#must_anwser").val()=="-1"){
                alert("请选择微信号限制条件");
                return false;
            }
            if($("#must_anwser").val()=="0"){
                if($("#question2_answer").val()==""||$("#question1_answer").val()==""||$("#question1").val()==-1||$("#question2").val()==-1){
                    alert("请选择问题并写下答案");
                    return false;
                }
            }
            if(confirm("您确定您的照片和微信号是正确有效的么？")){
                return true
            }else{
                return false
            }
            if (!checkSubmitFlg) {
                checkSubmitFlg = true;
                return true;
            }else{
                alert("不用重复提交啦");
                return false;
            }

        }
        function goback(){
            window.history.back();
        }
        var temp;
        var questions=eval($("#questions").val());
        $("#question1").change(function(){
            if(temp){
                $("#question2").append(temp);
            }
           temp=$("#question2 option[value='"+$("#question1").val()+"']").remove();
        });
        $("#question2").change(function(){
            if($("#question1").val()==-1){
                return alert("先选择问题一");
            }
        });
        $("#must_anwser").change(function(){
            if($(this).val()=="1"){
                $(".must_anwser").hide();
            }else{
                $(".must_anwser").show();
            }
        })
        //   var prev;
        //    $("#question1").change(function(obj){
        //       // alert($(this).val());
        //        if(!prev){
        //            $("#question2").append(prev);
        //            $("#question3").append(prev);
        //        }
        //        prev='<option value="+$(this).val()+">"+$(this).text()+"</option>';
        //        $("#question2 option[value='"+$(this).val()+"']").remove();
        //        $("#question3 option[value='"+$(this).val()+"']").remove();
        //
        //    });
    </script>

</div>
</div>
</body>
</html>